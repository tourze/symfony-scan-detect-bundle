{
  "name": "tourze/symfony-scan-detect-bundle",
  "description": "扫描检测",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "psr/simple-cache": "^1.0|^2.0|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\ScanDetectBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\ScanDetectBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  }
}