{
  "name": "tourze/symfony-scan-detect-bundle",
  "description": "Symfony bundle for detecting and preventing malicious scan attempts and 404 attacks using PSR-16 SimpleCache",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "psr/simple-cache": "^1.0|^2.0|^3.0",
    "symfony/cache": "^7.3",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-symfony-kernel-test": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\ScanDetectBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\ScanDetectBundle\\Tests\\": "tests"
    }
  }
}